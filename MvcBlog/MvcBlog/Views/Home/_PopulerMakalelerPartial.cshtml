@model IEnumerable<MvcBlog.Models.Makale>

@{
    Layout = null;
}

<h5 class="card-header">En Çok Okunan Makaleler</h5>
<div class="row">
    <div class="card-body">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="/Home/MakaleDetay/@item.MakaleID">@item.Baslik (@item.Okunma)</a></li>
            }
        </ul>
    </div>
</div>